# Alchemy Webhook Configuration (from Alchemy dashboard)
ALCHEMY_SIGNING_KEY=add_here

# Treasury wallet address that should receive deposits (must match the Node bot)
TREASURY_AGW_ADDRESS=add_here

# Node bot internal credit endpoint
NODE_INTERNAL_CREDIT_URL=http://127.0.0.1:3000/internal/credit

# Bearer used to call the Node bot internal endpoint
# Must match INTERNAL_BEARER in the Node bot .env
INTERNAL_BEARER=add_here
NODE_INTERNAL_BEARER=add_here

# Enable request and error logging to alchemy_deposits.log
WEBHOOK_DEBUG=true
